{
  "name": "expresso-gateway",
  "version": "2.1.0",
  "description": "api-gateway with express",
  "main": "src/server.ts",
  "scripts": {
    "key:generate": "npx ts-node ./src/scripts/generate.ts",
    "dev": "npm-run-all build start:dev",
    "start": "nodemon --exec 'ts-node' ./src/server.ts",
    "start:dev": "nodemon --exec node ./dist/server.js",
    "clean": "rm -rf ./dist",
    "test:lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "test:types": "tsc --noEmit",
    "test:types-watch": "npm run test:types -- --watch",
    "prebuild": "yarn && npm-run-all clean test:lint test:types",
    "build": "npx swc src -d dist",
    "build:ts": "npx tsc -p .",
    "deploy": "npm run build && pm2 reload ecosystem.config.js",
    "serve:sandbox": "NODE_ENV=sandbox node ./dist/server.js",
    "serve:production": "NODE_ENV=production node ./dist/server.js",
    "serve:sandbox-docker": "NODE_ENV=sandbox pm2-runtime ./dist/server.js",
    "serve:production-docker": "NODE_ENV=production pm2-runtime ./dist/server.js",
    "prerelease": "npm-run-all prebuild",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "api-gateway",
    "express",
    "typescript"
  ],
  "author": "masb0ymas",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14.x"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "chalk": "~4.1.2",
    "compression": "^1.7.4",
    "convert-excel-to-json": "^1.7.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-useragent": "^1.0.15",
    "googleapis": "^110.0.0",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "http-errors": "^2.0.0",
    "i18next": "^22.4.9",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-middleware": "^3.2.2",
    "ioredis": "^5.2.5",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.0",
    "nodemailer-mailgun-transport": "^2.1.5",
    "npm-run-all": "^4.1.5",
    "query-string": "^8.1.0",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "~3.1.2",
    "request-ip": "^3.3.0",
    "slugify": "^1.6.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.26",
    "@types/compression": "^1.7.2",
    "@types/convert-excel-to-json": "^1.7.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/express-useragent": "^1.0.2",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.2",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/node-cron": "^3.0.7",
    "@types/nodemailer": "^6.4.7",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "@types/redis": "^2.8.32",
    "@types/request-ip": "^0.0.37",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.0",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^27.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "_moduleAliases": {
    "@expresso/*": "dist/@expresso/*",
    "@config/*": "dist/config/*",
    "@controllers/*": "dist/controllers/*",
    "@database/*": "dist/database/*",
    "@jobs/*": "dist/jobs/*",
    "@middlewares/*": "dist/middlewares/*",
    "@routes/*": "dist/routes/*",
    "@scripts/*": "dist/scripts/*",
    "@views/*": "dist/views/*"
  }
}
